source "https://rubygems.org"

# Use the GitHub Pages gem to match the GitHub Pages build environment.
# This includes Jekyll and a set of whitelisted plugins like jekyll-seo-tag and jekyll-sitemap.
gem "github-pages", group: :jekyll_plugins

# Needed for local development on Ruby 3+
# GitHub Pages build environment also supports this implicitly.
gem "webrick", "~> 1.8"
